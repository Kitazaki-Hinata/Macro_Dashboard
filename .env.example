###############################################
# Macro Dashboard 环境变量示例 (.env.example)
# 复制为 .env 并按需填写/调整
###############################################

# --- 必填：API Keys ---
# 在 https://apps.bea.gov/api/signup/ 申请
bea="YOUR-BEA-API-KEY"

# 在 https://fredaccount.stlouisfed.org/apikeys 申请
fred="YOUR-FRED-API-KEY"

# 在 https://data.bls.gov/registrationEngine/ 申请
bls="YOUR-BLS-API-KEY"

# --- 并发/超时建议（可按机器/网络情况微调）---
# Yahoo Finance 建议低并发，避免被限流
YF_WORKERS=1

# BLS 请求并发与超时（秒）
BLS_WORKERS=2
BLS_POST_TIMEOUT=120

# BEA/FRED 并发（可选），默认代码内有保守值
#BEA_WORKERS=3
#FRED_WORKERS=3

# --- TradingEconomics 抓取参数 ---
# 是否显示浏览器（可视化抓取，有助于调试页面元素）
TE_SHOW_BROWSER=true

# 强制无头模式（与 TE_SHOW_BROWSER 二选一；true 则无界面）
TE_FORCE_HEADLESS=false

# 当未显式设置时的兜底开关（一般不需要同时设置）
TE_HEADLESS=false

# TE 抓取缓存（秒）。在缓存期内优先读缓存以减少重复抓取
TE_CACHE_TTL_SECONDS=900

# TE 并发相关（仅当批量抓取时生效；缺省即可）
#TE_WORKERS=2
#TE_POOL_SIZE=2

# 其他：如网络在国内，强烈建议使用稳定的代理/加速
# 例如：
# HTTPS_PROXY=http://127.0.0.1:7890
# HTTP_PROXY=http://127.0.0.1:7890
